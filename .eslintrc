{
  "parser": "babel-eslint",
  "extends": "airbnb",

  "rules": {
    "comma-dangle": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",

    "react/jsx-filename-extension": "off",
    "react/no-multi-comp": "off",
    "react/prefer-stateless-function": "off",
    "react/require-default-props": "off"
  }
}
